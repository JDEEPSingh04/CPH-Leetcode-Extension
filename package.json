{
  "name": "LH",
  "displayName": "Leetcode Helper",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "CPH.fetchProblem",
        "title": "Fetch Test Cases",
        "icon": "$(cloud-download)",
        "category": "CPH"
      },
      {
        "command": "CPH.runTestCases",
        "title": "Run Test Cases",
        "icon": "$(play)",
        "category": "CPH"
      },
      {
        "command": "CPH.addTestCase",
        "title": "Add Test Case",
        "icon": "$(add)",
        "category": "CPH"
      }
    ],
    "keybindings": [
      {
        "command": "CPH.fetchProblem",
        "key": "cmd+shift+f"
      },
      {
        "command": "CPH.runTestCases",
        "key": "cmd+shift+r",
        "when": "resourceFilename =~ /solution\\.(cpp|py|java|js)$/"
      },
      {
        "command": "CPH.addTestCase",
        "key": "cmd+shift+a",
        "when": "resourceFilename =~ /solution\\.(cpp|py|java|js)$/"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cph-sidebar",
          "title": "LeetCode Helper",
          "icon": "resources/leetcode.svg"
        }
      ]
    },
    "views": {
      "cph-sidebar": [
        {
          "type": "webview",
          "id": "cph-commands",
          "name": "Commands",
          "icon": "resources/leetcode.svg",
          "contextualTitle": "LeetCode Helper"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "fs": "^0.0.1-security"
  }
}
